{
  "hooks": {
    "Stop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "bash scripts/stop_hook_inbox.sh",
            "timeout": 10
          }
        ]
      }
    ]
  },
  "permissions": {
    "deny": [
      "Bash(rm -rf /)",
      "Bash(rm -rf /*)",
      "Bash(rm -rf /mnt/*)",
      "Bash(rm -rf /mnt/c/*)",
      "Bash(rm -rf /home/*)",
      "Bash(rm -rf ~*)",
      "Bash(sudo *)",
      "Bash(su *)",
      "Bash(kill *)",
      "Bash(killall *)",
      "Bash(pkill *)",
      "Bash(git push --force*)",
      "Bash(git push -f *)",
      "Bash(git reset --hard*)",
      "Bash(tmux kill-server*)",
      "Bash(tmux kill-session*)",
      "Bash(mkfs*)",
      "Bash(dd if=*)",
      "Bash(chmod -R * /)",
      "Bash(chown -R * /)"
    ]
  }
}